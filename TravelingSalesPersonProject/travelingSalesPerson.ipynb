    "def total_distance(distances, cities_visited):\n",
    "    running_total = 0\n",
    "    cities_visited_length = len(cities_visited)\n",
    "    \n",
    "    for i in range(cities_visited_length - 1):\n",
    "        city1 = cities_visited[i]  \n",
    "        city2 = cities_visited[i + 1]\n",
    "        \n",
    "        running_total += distances[city1][city2]\n",
    "    \n",
    "    running_total += distances[cities_visited[-1]][cities_visited[0]]\n",
    "    \n",
    "    return running_total\n",
    "        \n",
    "def backtracking(listCities, listDistances):  \n",
    "    best_cities_visited = []\n",
    "    best_distance = float('inf')  \n",
    "    \n",
    "    def recursion(cities_left, cities_visited, best_cities_visited, best_distance):\n",
    "        if not cities_left:\n",
    "            distance = total_distance(listDistances, cities_visited)\n",
    "            if distance < best_distance:\n",
    "                best_distance = distance\n",
    "                best_cities_visited = cities_visited.copy()\n",
    "        else:\n",
    "            for city in cities_left:\n",
    "                copy_of_cities_visited = cities_visited.copy()\n",
    "                copy_of_cities_left = cities_left.copy()\n",
    "                copy_of_cities_visited.append(city)\n",
    "                copy_of_cities_left.remove(city)\n",
    "                best_cities_visited, best_distance = recursion(copy_of_cities_left, copy_of_cities_visited, best_cities_visited, best_distance)\n",
    "        \n",
    "        return best_cities_visited, best_distance\n",
    "    \n",
    "    best_cities_visited, best_distance = recursion(listCities, [], best_cities_visited, best_distance)\n",
    "    return best_cities_visited, best_distance\n",
